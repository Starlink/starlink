<?xml version="1.0"?>
<!DOCTYPE component SYSTEM "componentinfo.dtd">
<!-- @configure_input@ -->

<component id="@PACKAGE@" support="S">
  <version>@PACKAGE_VERSION@</version>
  <path>docs/sug</path>
  <description>Starlink User Guide</description>
  <abstract><p>
    This is a general introduction to the Starlink Project for new
    users. You should also read the local guide for your site before
    attempting to use Starlink computers. This will tell you how to
    access your local machine and what facilities are available. As
    these documents cannot be updated very frequently, you should
    refer to the on-line files mentioned for current details of
    Starlink staff, sites, documents, and software.
    </p></abstract>
  <dependencies @STAR_DEPENDENCIES_ATTRIBUTES@>
    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>M D Lawden</name>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <copyright>
    <year>1997</year>
    <name>PPARC</name>
  </copyright>
</component>
