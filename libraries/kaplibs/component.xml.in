<?xml version="1.0"?>
<!DOCTYPE component SYSTEM "componentinfo.dtd">
<!-- @configure_input@ -->

<component id="@PACKAGE@" support="S">
  <version>@PACKAGE_VERSION@</version>
  <path>libraries/kaplibs</path>
  <description>KAPLIBS - Internal subroutines used within the KAPPA package.</description>
  <abstract><p>KAPLIBS is a package of Fortran subroutine 
libraries which were originally written as part of the KAPPA package (a
package of general-purpose image-processing and visualization tools).
KAPLIBS provides software developers with access to many of the internal
KAPPA routines, so that KAPPA-like applications can be written and built
independently of KAPPA.</p></abstract>
  <dependencies @STAR_DEPENDENCIES_ATTRIBUTES@>
    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>D.S. Berry</name>
      <uname>dsb</uname>
      <email>d.berry@jach.hawaii.edu</email>
      <role>owner</role>
    </person>
    <person>
      <name>Malcolm J. Currie</name>
      <uname>mjc</uname>
      <email>mjc@star.rl.ac.uk</email>
      <role>owner</role>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
