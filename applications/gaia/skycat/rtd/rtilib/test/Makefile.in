CC           	= @CC@
CXX           	= @CXX@
CFLAGS  	= @CXXDEBUGFLAGS@
TOPDIR      	= @DESTDIR@
LIBDIR      	= @LIBDIR@
DEPEND_TARGET	= @DEPEND_TARGET@

# Take include files from the build dirs, if present
BUILD_INCLUDES  = @TCLUTIL_BUILD_INCLUDES@ \
		  @ASTROTCL_BUILD_INCLUDES@

SRCS 		=  

OBJS 		=  

PROGS 		=  

INCLUDES	= -I../include \
		  $(BUILD_INCLUDES) \
		  -I$(TOPDIR)/include/astrotcl \
		  -I$(TOPDIR)/include/tclutil

CC_SWITCHES	= $(CFLAGS) $(DEFINES) $(INCLUDES)

TCLUTIL_LIB     = @TCLUTIL_LIB@
ASTROTCL_LIB     = @ASTROTCL_LIB@

LIBRARIES       = ../lib/librti.a \
		  $(ASTROTCL_LIB) \
		  $(TCLUTIL_LIB)

EXTRA_LIBS      = @LIBS@

DEPEND_FLAGS    = $(CC_SWITCHES)
SHELL    	= /bin/sh
AR       	= ar rc
RM       	= rm -f

all: 

test: $(PROGS)

lib:
	@cd ../src; $(MAKE)

All:
	cd ../..; $(MAKE); $(MAKE)

install:

clean: 
	$(RM) $(PROGS) $(OBJS) *\~ "#"* *.o

distclean: clean
	echo > make.dep
	rm -f Makefile	

