    MAN_DIR_SEPARATOR=""
    echo "$ac_t""man/man1 style" 1>&6
else
    MAN_DIR_SEPARATOR=""
    echo "$ac_t""WARNING: couldn't determine, assuming man/man1 style" 1>&6
