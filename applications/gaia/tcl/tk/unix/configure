ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT RANLIB ac_ct_RANLIB CPP EGREP CFLAGS_DEBUG CFLAGS_DEFAULT CFLAGS_OPTIMIZE CFLAGS_WARNING TK_DBGX DL_LIBS LD_FLAGS MATH_LIBS MAKE_LIB SHLIB_CFLAGS SHLIB_LD SHLIB_LD_LIBS SHLIB_SUFFIX SHLIB_VERSION TCL_BIN_DIR TCL_BUILD_LIB_SPEC TCL_DBGX TCL_LIB_FLAG TCL_SRC_DIR TCL_VERSION TK_BUILD_LIB_SPEC TK_CC_SEARCH_FLAGS TK_LD_SEARCH_FLAGS TK_LIB_FILE TK_LIB_FLAG TK_LIB_SPEC TK_MAJOR_VERSION TK_MINOR_VERSION TK_PATCH_LEVEL TK_SHLIB_CFLAGS TK_SRC_DIR TK_VERSION XINCLUDES XLIBSW TK_SHARED_BUILD LIBOBJS LTLIBOBJS'
