#========================================================================
# Rules for Fortran compilation, simple and generic
#========================================================================
# Note these would be simpler if $(<:.gen=.f) worked on Tru64 UNIX with
# a VPATH set (loses subdirectory in name).

.f.o:
	$(FC) -c $(FCFLAGS) $(STAR_FCFLAGS) $<

.gen.o:
	$(FGENERIC) -t n $<
	f=`echo $< | sed 's:\.gen:\.f:'` ;\
	$(FC) -c $(FCFLAGS) $(STAR_FCFLAGS) $$f ;\
	rm -f $$f

.gsc.o:
	$(FGENERIC) -t rlidc $<
	f=`echo $< | sed 's:\.gsc:\.f:'` ;\
	$(FC) -c $(FCFLAGS) $(STAR_FCFLAGS) $$f ;\
	rm -f $$f

.gsn.o:
	$(FGENERIC) -t rlid $<
	f=`echo $< | sed 's:\.gsn:\.f:'` ;\
	$(FC) -c $(FCFLAGS) $(STAR_FCFLAGS) $$f ;\
	rm -f $$f

