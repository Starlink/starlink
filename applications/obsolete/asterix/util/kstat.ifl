Interface KSTAT
   Parameter INP
      Position    1
      Type        'UNIV'
      ppath       'GLOBAL'
      Association '<->GLOBAL.BINDS'
      Prompt      'Enter input dataset'
      Help        'This must be 1 dimensional.'
   EndParameter

   Parameter INP2
      Position    2
      Type        'UNIV'
      Prompt      'Enter another primitive array of the same size'
      Help        'This will be correlated with the first'
   EndParameter
EndInterface
